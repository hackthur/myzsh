      black: '#343d46'
      red: '#EC5f67'
      green: '#99C794'
      yellow: '#FAC863'
      blue: '#6699cc'
      magenta: '#c594c5'
      cyan: '#5fb3b3'
      white: '#d8dee9'

    # Bright colors
